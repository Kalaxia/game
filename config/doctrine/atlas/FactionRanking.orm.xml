<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
	<entity name="App\Modules\Atlas\Model\FactionRanking" table="factionRanking">
		<id name="id" column="id" type="uuid" />

		<many-to-one field="ranking" target-entity="App\Modules\Atlas\Model\Ranking">

		</many-to-one>

		<many-to-one field="faction" target-entity="App\Modules\Demeter\Model\Color">
			
		</many-to-one>
		
		<field name="points" type="smallint" />
		<field name="pointsPosition" type="smallint" />
		<field name="pointsVariation" type="smallint" />
		<field name="newPoints" type="smallint" />
		<field name="general" type="smallint" />
		<field name="generalPosition" type="smallint" />
		<field name="generalVariation" type="smallint" />
		<field name="wealth" type="smallint" />
		<field name="wealthPosition" type="smallint" />
		<field name="wealthVariation" type="smallint" />
		<field name="territorial" type="smallint" />
		<field name="territorialPosition" type="smallint" />
		<field name="territorialVariation" type="smallint" />
		<field name="createdAt" type="datetime_immutable" />
	</entity>
</doctrine-mapping>
