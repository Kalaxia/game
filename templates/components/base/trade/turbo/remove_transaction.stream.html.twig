<turbo-stream action="remove" target="commercial-shipping-{{ commercial_shipping.id }}">
	<!-- The element with DOM ID "message_1" will be removed.
	The contents of this stream element are ignored. -->
</turbo-stream>

<turbo-stream action="replace" target="commercial-shipping-available-ships-count">
	<template>
		<twig:CommercialShippingAvailableShipsCount :maxShips="max_ships" :usedShips="used_ships" />
	</template>
</turbo-stream>
